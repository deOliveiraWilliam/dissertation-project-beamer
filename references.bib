  doi             = {10.1016/j.jacc.2011.06.066},
  file            = {:home/william/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Koo et al. - 2011 - Diagnosis of ischemia-causing coronary stenoses by noninvasive fractional flow reserve computed from coronary comput.pdf:pdf},
  issn            = {15583597},
  journal         = {Journal of the American College of Cardiology},
  keywords        = {computational fluid dynamics,coronary CT angiography,fractional flow reserve},
  mendeley-groups = {masters/CFD/Usage examples},
