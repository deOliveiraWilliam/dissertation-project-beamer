  month           = {nov},
  pages           = {305--333},
  publisher       = {Cambridge University Press},
  title           = {{A Herschel-Bulkley model for mud flow down a slope}},
  url             = {https://www.cambridge.org/core/journals/journal-of-fluid-mechanics/article/abs/herschelbulkley-model-for-mud-flow-down-a-slope/AE9E17581B6DAC8AEEB37FBC7B3FC63B},
