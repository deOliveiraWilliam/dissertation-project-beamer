  author          = {Vuorinen, Ville and Aarnio, Mia and Alava, Mikko and Alopaeus, Ville and Atanasova, Nina and Auvinen, Mikko and Balasubramanian, Nallannan and Bordbar, Hadi and Er{\"{a}}st{\"{o}}, Panu and Grande, Rafael and Hayward, Nick and Hellsten, Antti and Hostikka, Simo and Hokkanen, Jyrki and Kaario, Ossi and Karvinen, Aku and Kivist{\"{o}}, Ilkka and Korhonen, Marko and Kosonen, Risto and Kuusela, Janne and Lestinen, Sami and Laurila, Erkki and Nieminen, Heikki J. and Peltonen, Petteri and Pokki, Juho and Puisto, Antti and R{\aa}back, Peter and Salmenjoki, Henri and Sironen, Tarja and {\"{O}}sterberg, Monika},
  doi             = {10.1016/j.ssci.2020.104866},
