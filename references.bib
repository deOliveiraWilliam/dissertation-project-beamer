@article{Chapman2006,
  abstract        = {The problem of irrotational inviscid incompressible free-surface flow is examined in the limit of small Froude number. Since this is a singular perturbation, singularities in the flow field (or its analytic continuation) such as stagnation points, or corners in submerged objects or on rough beds, lead to a divergent asymptotic expansion, with associated Stokes lines. Recent techniques in exponential asymptotics are employed to observe the switching on of exponentially small gravity waves across these Stokes lines. As a concrete example, the flow over astep is considered. It is found that there are three possible parameter regimes, depending on whether the dimensionless step height is small, of the same order, or large compared to the square of the Froude number. Asymptotic results are derived in each case, and compared with numerical simulations of the full nonlinear problem. The agreement is remarkably good, even at relatively large Froude number. This is in contrast to the alternative analytical theory of small step height, which is accurate only for very small steps. {\textcopyright} 2006 Cambridge University Press.},
  author          = {Chapman, S. Jonathan and Vanden-Broeck, Jean Marc},
  doi             = {10.1017/S0022112006002394},
  issn            = {14697645},
  journal         = {Journal of Fluid Mechanics},
  mendeley-groups = {masters/Obstacles},
  pages           = {299--326},
  publisher       = {Cambridge University Press},
  title           = {{Exponential asymptotics and gravity waves}},
  url             = {https://www.cambridge.org/core/journals/journal-of-fluid-mechanics/article/abs/exponential-asymptotics-and-gravity-waves/9DBFB9F9B020C19E994D554809642CB7},
  volume          = {567},
  year            = {2006}
}
@article{Chardard2011,
  abstract        = {Free-surface flows past submerged obstacles in a channel are considered. The fluid is assumed to be inviscid and incompressible and the flow to be irrotational. The first-order approximation of long nonlinear surface waves over one or two bumps results in a forced Korteweg-de Vries (fKdV) equation. Solutions of the stationary fKdV equation are constructed and their stability is studied, either analytically or numerically. These various solutions include solitary waves over a single bump, solitary waves with two humps over a double bump, table-top solutions over a double bump and fronts.},
  author          = {Chardard, Fr{\'{e}}d{\'{e}}ric and Dias, Fr{\'{e}}d{\'{e}}ric and {Yen Nguyen}, Hai and Vanden-Broeck, Jean-Marc and Dias, F and {Vanden-Broeck Chardard}, J-m F and Nguyen, H Y and Vanden-Broeck, J-m},
  doi             = {10.1007/s10665-010-9424-6},
  journal         = {J Eng Math},
  keywords        = {Forced,Free-surface flows {\textperiodcentered},Korteweg-de,Numerical simulations {\textperiodcentered},Stability {\textperiodcentered},Topography,Vries equation {\textperiodcentered}},
  mendeley-groups = {masters/Obstacles},
